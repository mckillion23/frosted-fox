{
    "name": "frosted-fox",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "12.14.1",
        "npm": "7.0.15"
    },
    "dependencies": {
        "autoprefixer": "^9.8.6",
        "emailjs-com": "^2.6.4",
        "prismic-javascript": "^3.0.2",
        "react": "^16.13.1",
