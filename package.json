{
    "name": "frosted-fox",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "12.14.1",
        "npm": "7.0.15"
    },
    "dependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "autoprefixer": "^9.8.6",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.1",
        "tailwindcss": "^1.6.2"
    },
    "scripts": {
        "start": "npm run watch:css && react-scripts start",
        "build": "npm run build:css && react-scripts build",
